<%= form_with model: @tag, local: true do |f| %>
  <%= render 'shared/error_messages', model: f.object %>

  <div class="field">
    <label class="form-name">タグの名前</label>
      <%= f.text_field :name, class:"form-default", maxlength:"20", id:"tag_name", autofocus: true %>
  </div>

  <div class="actions">
    <%= f.submit "登録する", class: :form__btn  %>
  </div>
<% end %>